{
  "scriptFile": "__init__.py",
  "bindings": [ 
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "inputblob",
      "type": "blob",
      "dataType": "binary",
      "path": "recordings/{Audio.FileName}",
      "connection": "MyStorageConnectionAppSetting",
      "direction": "in"
    }
  ]
}